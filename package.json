{
  "name": "mangatown-data",
  "version": "1.0.0",
  "main": "dist/index.js",
  "license": "MIT",
  "scripts": {
    "prestart": "npm run -s build",
    "start": "node dist/index.js",
    "dev": "nodemon src/index.js --exec \"node -r dotenv/config -r babel-register\"",
    "clean": "rimraf dist",
    "build": "npm run clean && mkdir -p dist && babel src -s -D -d dist",
    "test": "npm run lint && jest --coverage && cat ./coverage/lcov.info | coveralls",
    "test-w": "jest --watch",
    "lint": "esw src test",
    "lint-w": "esw -w src test",
    "prepublish": "npm build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AzSiAz/MangaTown-Data.git"
  },
  "keywords": [
    "Mangatown",
    "manga"
  ],
  "author": "AzSiAz <contact@azsiaz.tech> (azsiaz.tech)",
  "bugs": {
    "url": "https://github.com/AzSiAz/MangaTown-Data/issues"
  },
  "homepage": "https://github.com/AzSiAz/MangaTown-Data#readme",
  "dependencies": {
    "bluebird": "^3.5.0",
    "cheerio": "^0.22.0",
    "isomorphic-fetch": "^2.2.1"
  },
  "devDependencies": {
    "@types/jest": "^19.2.2",
    "babel-cli": "^6.24.0",
    "babel-jest": "^19.0.0",
    "babel-preset-env": "^1.2.1",
    "babel-register": "^6.24.0",
    "coveralls": "^2.12.0",
    "dotenv": "^4.0.0",
    "eslint": "^3.17.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jest": "^19.0.1",
    "eslint-watch": "^3.0.1",
    "jest": "^19.0.2",
    "nodemon": "^1.11.0",
    "rimraf": "^2.6.1"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "node": "current"
          }
        }
      ]
    ]
  },
  "eslintConfig": {
    "plugins": [
      "import",
      "jest"
    ],
    "parserOptions": {
      "ecmaVersion": 2017,
      "sourceType": "module"
    },
    "env": {
      "es6": true,
      "node": true,
      "jest": true
    },
    "extends": [
      "eslint:recommended"
    ],
    "rules": {
      "no-console": 0,
      "jest/no-focused-tests": 2,
      "jest/no-identical-title": 2
    }
  },
  "jest": {
    "testEnvironment": "node"
  }
}
