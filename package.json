{
  "name": "mangatown-data",
  "description": "Get data from MangaTown",
  "version": "1.1.3",
  "main": "dist/index.js",
  "license": "MIT",
  "engines": {
    "node": ">=6.0.0",
    "npm": ">=3.10.0"
  },
  "scripts": {
    "prestart": "npm run -s build",
    "start": "node dist/index.js",
    "dev": "nodemon src/index.js --exec \"node -r dotenv/config -r babel-register\"",
    "clean": "rimraf dist",
    "build": "npm run clean && mkdir -p dist && babel src -s -D -d dist",
    "test": "npm run lint && jest --coverage --runInBand && cat ./coverage/lcov.info | coveralls",
    "test-w": "jest --watch --runInBand",
    "lint": "esw src test",
    "lint-w": "esw -w src test",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AzSiAz/MangaTown-Data.git"
  },
  "keywords": [
    "Mangatown",
    "manga"
  ],
  "author": "AzSiAz <contact@azsiaz.tech> (azsiaz.tech)",
  "bugs": {
    "url": "https://github.com/AzSiAz/MangaTown-Data/issues"
  },
  "homepage": "https://github.com/AzSiAz/MangaTown-Data#readme",
  "dependencies": {
    "async": "^2.1.5",
    "bluebird": "^3.5.0",
    "cheerio": "^0.22.0",
    "isomorphic-fetch": "^2.2.1"
  },
  "devDependencies": {
    "@types/jest": "^19.2.2",
    "babel-cli": "^6.24.0",
    "babel-jest": "^19.0.0",
    "babel-preset-env": "^1.2.2",
    "babel-register": "^6.24.0",
    "coveralls": "^2.12.0",
    "dotenv": "^4.0.0",
    "eslint": "^3.17.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jest": "^19.0.1",
    "eslint-watch": "^3.0.1",
    "jest": "^19.0.2",
    "nodemon": "^1.11.0",
    "rimraf": "^2.6.1"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "node": 6
          },
          "useBuiltIns": true
        }
      ]
    ]
  },
  "eslintConfig": {
    "plugins": [
      "import",
      "jest"
    ],
    "parserOptions": {
      "ecmaVersion": 2017,
      "sourceType": "module"
    },
    "env": {
      "es6": true,
      "node": true,
      "jest": true
    },
    "extends": [
      "eslint:recommended"
    ],
    "rules": {
      "no-console": 0,
      "jest/no-focused-tests": 2,
      "jest/no-identical-title": 2
    }
  },
  "jest": {
    "testEnvironment": "node"
  }
}
