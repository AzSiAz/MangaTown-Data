{
  "name": "mangatown-data",
  "description": "Get data from MangaTown",
  "version": "2.0.1",
  "main": "src/index.js",
  "license": "MIT",
  "engines": {
    "node": ">=10.0.0",
    "npm": ">=3.10.0"
  },
  "scripts": {
    "test": "npm run lint && jest --coverage --runInBand",
    "test-w": "jest --watch --runInBand",
    "lint": "esw src tests",
    "lint-w": "esw -w src tests"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AzSiAz/MangaTown-Data.git"
  },
  "keywords": [
    "Mangatown",
    "manga"
  ],
  "author": "AzSiAz <contact@azsiaz.tech> (azsiaz.tech)",
  "bugs": {
    "url": "https://github.com/AzSiAz/MangaTown-Data/issues"
  },
  "homepage": "https://github.com/AzSiAz/MangaTown-Data#readme",
  "dependencies": {
    "async": "2.6.2",
    "cheerio": "1.0.0-rc.2",
    "isomorphic-fetch": "2.2.1"
  },
  "devDependencies": {
    "@types/jest": "24.0.11",
    "eslint": "5.15.3",
    "eslint-plugin-import": "2.16.0",
    "eslint-plugin-jest": "22.4.1",
    "eslint-watch": "5.0.1",
    "jest": "24.5.0"
  },
  "eslintConfig": {
    "plugins": [
      "jest"
    ],
    "parserOptions": {
      "ecmaVersion": 2018,
      "sourceType": "module"
    },
    "env": {
      "es6": true,
      "node": true,
      "jest": true
    },
    "extends": [
      "eslint:recommended"
    ],
    "rules": {
      "no-console": 0,
      "jest/no-focused-tests": 2,
      "jest/no-identical-title": 2
    }
  },
  "jest": {
    "testEnvironment": "node"
  }
}